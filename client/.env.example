# =================================
# VeeFore Client Environment Variables Template
# =================================
# Copy this file to .env and update with your actual values

# ===== CLIENT CONFIGURATION =====
VITE_API_URL=http://localhost:5000
VITE_APP_NAME=VeeFore
VITE_APP_VERSION=1.0.0

# ===== FIREBASE CLIENT CONFIGURATION =====
VITE_FIREBASE_API_KEY=AIzaSyDemo-API-Key-Replace-With-Actual-Key
VITE_FIREBASE_AUTH_DOMAIN=demo-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=demo-project-id
VITE_FIREBASE_STORAGE_BUCKET=demo-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789012
VITE_FIREBASE_APP_ID=1:123456789012:web:demo_app_id

# ===== PAYMENT GATEWAYS (PUBLIC KEYS) =====
VITE_STRIPE_PUBLIC_KEY=pk_test_demo1234567890abcdefghijklmnopqrstuvwxyz
VITE_RAZORPAY_KEY_ID=rzp_test_demo1234567890

# ===== ANALYTICS & MONITORING =====
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
VITE_MIXPANEL_TOKEN=demo_mixpanel_token

# ===== FEATURE FLAGS =====
VITE_ENABLE_SOCIAL_PUBLISHING=true
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_VIDEO_GENERATION=true
VITE_ENABLE_TEAM_FEATURES=true

# ===== ENVIRONMENT SETTINGS =====
VITE_ENV=development
VITE_DEBUG=true

# ===== DOMAIN CONFIGURATION =====
VITE_BASE_URL=http://localhost:5000
VITE_DOMAIN=localhost

